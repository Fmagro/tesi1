<h1>Listing Concerts</h1>
<%= link_to 'New concert_song', new_concert_song_path %>
<table>
  <tr>
    <th>Concert</th>
    <th>Song</th>
    <th colspan="3"></th>
  </tr>
 
  <% @concert_songs.each do |concert_song| %>
    <tr>
      <td><%= concert_song.concert.name %></td>
      <td><%= concert_song.song.title %></td>
      <td><%= link_to 'Show', concert_song_path(concert_song) %></td>
      <td><%= link_to 'Edit', edit_concert_song_path(concert_song) %></td>
      <td><%= link_to 'Destroy', concert_song_path(concert_song),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Back', welcome_index_path %>
