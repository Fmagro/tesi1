<h1>Listing Concerts</h1>
<%= link_to 'New concert', new_concert_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Date</th>
    <th colspan="3"></th>
  </tr>
 
  <% @concerts.each do |concert| %>
    <tr>
      <td><%= concert.name %></td>
      <td><%= concert.date %></td>
      <td><%= link_to 'Show', concert_path(concert) %></td>
      <td><%= link_to 'Edit', edit_concert_path(concert) %></td>
      <td><%= link_to 'Destroy', concert_path(concert),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Back', welcome_index_path %>
