<h1>Listing Concerts and Songs</h1>
<%= link_to 'New setlist', new_setlist_path %>
<table>
  <tr>
    <th>Concert</th>
    <th>Song</th>
    <th colspan="3"></th>
  </tr>
 
  <% @setlists.each do |setlist| %>
    <tr>
      <td><%= setlist.concert.name %></td>
      <td><%= setlist.song.title %></td>
      <td><%= link_to 'Show', setlist_path(setlist) %></td>
      <td><%= link_to 'Edit', edit_setlist_path(setlist) %></td>
      <td><%= link_to 'Destroy', setlist_path(setlist),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Back', welcome_index_path %>
